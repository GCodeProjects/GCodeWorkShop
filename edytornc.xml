    <defaultLanguage>auto</defaultLanguage>
    <allowLanguageSelection>1</allowLanguageSelection>
    <postInstallationActionList>
        <associateWindowsFileExtension>
            <extensions>.nc</extensions>
            <friendlyName></friendlyName>
            <icon>${installdir}\ncfile.ico</icon>
            <mimeType>text/nc</mimeType>
            <progID>edytornc</progID>
            <commandList>
                <command>
                    <makeDefault>1</makeDefault>
                    <runProgram>${installdir}\edytornc.exe</runProgram>
                    <runProgramArguments>"%1"</runProgramArguments>
                    <verb>open</verb>
                </command>
            </commandList>
            <ruleList>
                <platformTest>
                    <type>windows</type>
                </platformTest>
            </ruleList>
        </associateWindowsFileExtension>
        <createSymLink>
            <linkName>~/bin/edytornc</linkName>
            <target>${installdir}/edytornc</target>
            <ruleList>
                <fileExists>
                    <path>~/bin</path>
                </fileExists>
                <platformTest>
                    <type>linux</type>
                </platformTest>
                <fileExists>
                    <path>${installdir}/edytornc</path>
                </fileExists>
            </ruleList>
        </createSymLink>
    </postInstallationActionList>
    <postUninstallationActionList>
        <removeWindowsFileAssociation>
            <extensions>.nc</extensions>
            <mimeType>text/nc</mimeType>
            <progID>edytornc</progID>
        </removeWindowsFileAssociation>
        <registryDelete>
            <key>HKEY_LOCAL_MACHINE\SOFTWARE\EdytorNC</key>
            <name></name>
            <ruleList>
                <compareText>
                    <logic>equals</logic>
                    <text>${remove_settings}</text>
                    <value>remove_settings</value>
                </compareText>
            </ruleList>
        </registryDelete>
        <deleteFile>
            <path>~/bin/edytornc</path>
            <ruleList>
                <platformTest>
                    <type>linux</type>
                </platformTest>
                <fileExists>
                    <path>~/bin/edytornc</path>
                </fileExists>
            </ruleList>
        </deleteFile>
        <registryDelete>
            <key>HKEY_CURRENT_USER\Software\EdytorNC</key>
            <name></name>
        </registryDelete>
        <showQuestion>
            <default>yes</default>
            <text>Remove settings from registry</text>
            <variable>remove_settings</variable>
        </showQuestion>
    </postUninstallationActionList>
    <allowedLanguages>en pl</allowedLanguages>
    <enableRollback>1</enableRollback>
    <enableTimestamp>1</enableTimestamp>
